# Production Environment Variables for TG App FSRS Frontend

# API Configuration
VITE_API_BASE_URL=https://tgapp-fsrs-backend.onrender.com
VITE_API_TIMEOUT=15000

# Application Configuration
VITE_APP_NAME=TG App FSRS
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=production

# Performance Configuration
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_ANALYTICS=true

# Feature Flags
VITE_ENABLE_FSRS=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_PWA=true
VITE_ENABLE_SERVICE_WORKER=true

# Security Configuration
VITE_ENABLE_CSP=true
VITE_ENABLE_HTTPS_ONLY=true

# Caching Configuration
VITE_CACHE_VERSION=1.0.0
VITE_ENABLE_AGGRESSIVE_CACHING=true

# Monitoring Configuration
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=
VITE_HOTJAR_ID=

# Build Configuration
VITE_BUILD_TIMESTAMP=
VITE_GIT_COMMIT_HASH=
VITE_DEPLOYMENT_VERSION=1.0.0